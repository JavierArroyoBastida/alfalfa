{
  "containerDefinitions": [
    {
      "name": "worker",
      "image": "431265468707.dkr.ecr.us-west-1.amazonaws.com/alfalfa_worker:latest",
      "cpu": 0,
      "memory": 400,
      "essential": true,
      "environment": [
          {
              "name": "JOB_QUEUE_URL",
              "value": "https://sqs.us-west-1.amazonaws.com/431265468707/alfalfa"
          },
          {
            "name": "MONGO_URL",
            "value": "<%= @MONGO_URL %>"
          },
          {
            "name": "MONGO_DB_NAME",
            "value": "alfalfa"
          }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-worker",
          "awslogs-region": "us-west-1"
        }
      }
    }
  ],
  "family": "worker",
  "taskRoleArn": "arn:aws:iam::431265468707:role/ecsRole"
}

