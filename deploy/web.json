{
  "containerDefinitions": [
    {
      "name": "web",
      "image": "431265468707.dkr.ecr.us-west-1.amazonaws.com/alfalfa_web:latest",
      "cpu": 0,
      "memory": 512,
      "essential": true,
      "environment": [
          {
              "name": "JOB_QUEUE_URL",
              "value": "https://sqs.us-west-1.amazonaws.com/431265468707/alfalfa"
          },
          {
              "name": "NODE_ENV",
              "value": "production"
          },
          {
            "name": "MONGO_URL",
            "value": "<%= @MONGO_URL %>"
          }
      ],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "family": "web",
  "taskRoleArn": "arn:aws:iam::431265468707:role/ecsRole"
}

